		},
		"homepage": "https://github.com/xpresspopup/backend#readme",
		"dependencies": {
				"@babel/polyfill": "^7.2.5",
				"agenda": "^2.0.2",
				"bcryptjs": "^2.4.3",
				"body-parser": "^1.19.0",
				"cors": "^2.8.5",
				"dotenv": "^6.2.0",
				"express": "^4.16.4",
				"joi": "^14.3.0",
				"jsonwebtoken": "^8.5.1",
				"mongodb": "^3.3.0",
				"mongoose": "*",
